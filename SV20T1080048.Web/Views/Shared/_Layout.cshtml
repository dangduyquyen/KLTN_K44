    <title>OanhVuSpa</title>
    
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/SV20T1080048.Web.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/font/fontawesome-free-6.5.1/css/all.css">
    <link rel="stylesheet" href="~/themes/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/_header.css">
    <link rel="stylesheet" href="~/css/_footer.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/_dichvu.css">
    <link rel="stylesheet" href="~/css/_gioithieu.css">
